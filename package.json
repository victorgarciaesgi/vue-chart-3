  "scripts": {
    "clean": "rimraf dist && rimraf types",
    "compile": "tsc --pretty -p tsconfig.build.json && cp -r ./dist ./debug/vue2/node_modules/vue-chart-3 && cp -r ./types ./debug/vue2/node_modules/vue-chart-3",
    "dev": "tsc --pretty --watch",
    "build": "yarn run clean && yarn run compile",
    "debug": "cd debug/vue2 && yarn serve",
    "publish:legacy": "npm publish --tag legacy",
    "test": "karma start",
    "patch": "npm version patch --no-git-tag-version",
    "minor": "npm version minor --no-git-tag-version",
    "major": "npm version major --no-git-tag-version"
  },
